resource/name = "TestMap.gd"
script/source = "extends Node2D

onready var player
var timer = 0

func _ready():
	player = Global.player
	set_process(true)
	pass

func _process(delta):
	if player.estado == player.STATE.FALLING:
		timer += delta
	else:
		timer = 0
	if timer > 5:
		get_tree().reload_current_scene()
		timer = 0
	pass"

